<!-- feed starts -->
<div class="container">
  <%= render "form" %>
  <div id="timeline">
    <%= render partial: "post", collection: @posts %>
  </div>
</div>
<!-- feed ends -->

<!-- widgets starts -->
<div>
     <div class="widgets">
         <div class="widgets__input">
           <span class="material-symbols-outlined"> search
           </span>        
           <input type="text" placeholder="Search Twitter">
         </div>
        <div class="widgets__widgetContainer">
            <h2>What's happening?</h2>
            <div class="twitter-tweet twitter-tweet-rendered" style="display: flex; max-width: 550px; width: 100%; margin-top: 10px; margin-bottom: 10px;">
                <iframe id="twitter-widget-0" scrolling="no" frameborder="0" allowtransparency="true" allowfullscreen="true" class="" style="position: static; visibility: visible; width: 250px; height: 401px; display: block; flex-grow: 1;" title="Twitter Tweet" src="https://platform.twitter.com/embed/Tweet.html?dnt=false&amp;embedId=twitter-widget-0&amp;&amp;frame=false&amp;hideCard=false&amp;hideThread=false&amp;id=463440424141459456&amp;lang=en&amp;origin=https%3A%2F%2Fsomanath-goudar.github.io%2Fhtml-css-projects%2Ftwitter-clone%2F&amp;sessionId=2249c103d2a0a470ab61afb43435bc472940d581&amp;theme=light&amp;widgetsVersion=a3525f077c700%3A1667415560940&amp;width=550px" data-tweet-id="463440424141459456"></iframe>
            </div>
            <script async="" src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
        </div>
    </div>
</div>
<!-- widgets ends -->


  

  
</div>

